{
  "name": "onlineweb4",
  "version": "1.0.0",
  "description": "Onlineweb 4",
  "browserslist": [
    "last 2 versions",
    "> 5% in NO"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "corejs-typeahead": "^1.1.0",
    "cropperjs": "^1.6.1",
    "dropzone": "^4.3.0",
    "eonasdan-bootstrap-datetimepicker": "^4.17.43",
    "file-loader": "^6.2.0",
    "font-awesome": "^4.7.0",
    "fuse.js": "^2.7.3",
    "imports-loader": "^1.2.0",
    "jquery": "2",
    "jquery-bar-rating": "^1.2.1",
    "js-cookie": "^3.0.5",
    "masonry-layout": "^4.1.1",
    "mini-css-extract-plugin": "1.6.2",
    "moment": "^2.29.2",
    "null-loader": "^4.0.1",
    "postcss-loader": "^4.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-bootstrap": "0.33.1",
    "react-dom": "^16.8.6",
    "react-markdown": "^2.5.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-transition-group": "^4.0.0",
    "showdown": "^1.9.1",
    "spin.js": "^2.3.2",
    "tablesorter": "^2.28.3",
    "underscore": "^1.12.1",
    "url-loader": "^4.1.1",
    "webpack": "^4.47.0",
    "webpack-cli": "^4.10.0",
    "webpack-bundle-tracker": "^2.0.1",
    "webpack-dev-server": "^4.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^5.2.7",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-webpack": "^0.13.7",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "less": "^4.2.0",
    "less-loader": "^7.3.0",
    "stylelint": "^15.10.0",
    "stylelint-config-standard": "^34.0.0"
  },
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --config webpack.production.config.js",
    "build:test": "webpack --config webpack.test.config.js",
    "dev": "node webpack.server.js",
    "start": "npm run dev",
    "lint": "npm run lint-js && npm run lint-less",
    "lint-js": "eslint assets/",
    "lint-less": "stylelint \"assets/**/*.less\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotkom/onlineweb4.git"
  },
  "author": "dotkom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotkom/onlineweb4/issues"
  },
  "homepage": "https://online.ntnu.no/",
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.js"
        }
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-list-comma-newline-after": "always-multi-line"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  }
}
