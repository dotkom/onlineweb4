<clientConfig version="1.1">
  <emailProvider id="online.ntnu.no">
    <domain>online.ntnu.no</domain>
    <displayName>Student Union Online email</displayName>
    <displayShortName>Online Student</displayShortName>
    <incomingServer type="imap">
      <hostname>imap.online.ntnu.no</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <authentication>password-cleartext</authentication>
      <username>%EMAILLOCALPART%</username>
    </incomingServer>
      <outgoingServer type="smtp">
      <hostname>smtp.online.ntnu.no</hostname>
      <port>465</port>
      <socketType>SSL</socketType>
      <authentication>password-cleartext</authentication>
      <username>%EMAILLOCALPART%</username>
    </outgoingServer>
    <documentation url="https://wiki.online.ntnu.no/projects/komiteer/wiki/Tjenester">
      <descr lang="no">Wikihjelp for tjenestene til komiteer</descr>
      <descr lang="en">Wikihelp for services for Online committee members</descr>
    </documentation>
  </emailProvider>
</clientConfig>
